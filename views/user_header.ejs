<head>
    <link rel="stylesheet" href="/public/assets/css/user_headers.min.css" type="text/css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
    <input type="hidden" id="url" value="<%= process.env.NODE_URL %>">
    <title>
        <%= header.title %>
    </title>
</head>

<body class="bg-dark text-white">
    <div class="title text-center mt-5">
        <h1 class="ms-5 text-white">
            <%= header.title %>
        </h1>
    </div>

    <div class="btns d-flex justify-content-between mt-5">
        <div class="contact d-flex">
            <button type="button" id="group_btn" class="btn btn-primary p-3 ms-5 d-inblock">Group</button>
            <button type="button" id="contact_btn" class="btn btn-primary p-3 ms-5 d-inblock">Contacts</button>
        </div>
        <div class="user text-center me-5">
            <h3>WELCOME <br>
                <%= data.name %>
            </h3>
        </div>
        <div class="logout me-5">
            <button type="button" id="logout_btn" class="btn btn-danger p-3 me-5">Logout</button>
        </div>
    </div>